{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "react_multi_1",
            "image": "kellydouglass/react_multi_1",
            "hostname": "react_multi_1",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "containerPort": 3000
                }
            ]
        },
        {
            "name": "server_multi_1",
            "image": "kellydouglass/rserver_multi_1",
            "hostname": "server_multi_1",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 4000,
                    "containerPort": 4000
                }
            ]
        },
        {
            "name": "nginx",
            "image": "kellydouglass/nginx_multi_1",
            "hostname": "nginx_multi_1",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}